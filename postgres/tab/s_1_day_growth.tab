DROP TABLE s_1_day_growth;
CREATE TABLE s_1_day_growth
(
    stock           CHAR(10),
    s_date          TIMESTAMP,
    s_close         NUMERIC,
    s_close_start   NUMERIC,
    s_growth        NUMERIC
);